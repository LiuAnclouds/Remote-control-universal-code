<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.kk_simple.MainActivity">

    <Button
        android:id="@+id/sdk_global_flow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="全球非压缩对码流程演示" />

    <Button
        android:id="@+id/sdk_gloabl_zip_flow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="全球压缩对码流程演示" />

    <Button
        android:id="@+id/sdk_flow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="大陆非压缩对码流程演示" />

    <Button
        android:id="@+id/sdk_zip_flow"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="大陆压缩对码流程演示" />

    <Button
        android:id="@+id/sdk_match_lineup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="匹配频道表" />

    <Button
        android:id="@+id/sdk_ac"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="空调面板" />
    <!--    <Button-->
    <!--        android:id="@+id/sdk_offline_db"-->
    <!--        android:layout_width="match_parent"-->
    <!--        android:layout_height="wrap_content"-->
    <!--        android:onClick="onClick"-->
    <!--        android:text="离线DB使用示例"/>-->
    <Button
        android:id="@+id/sdk_offline_xml"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="离线XML空调解码示例" />

</LinearLayout>